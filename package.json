{
	"name": "floatplane",
	"version": "4.1.1",
	"description": "Library for interfacing with the LMG Floatplane.com api",
	"main": "index.js",
	"types": "index.d.ts",
	"type": "module",
	"dependencies": {
		"cross-env": "^7.0.3",
		"got": "^12.4.1",
		"tough-cookie": "^4.1.2"
	},
	"devDependencies": {
		"@inrixia/helpers": "^2.0.2",
		"@types/tough-cookie": "^4.0.2",
		"@types/tough-cookie-file-store": "^2.0.1",
		"@typescript-eslint/eslint-plugin": "^5.37.0",
		"@typescript-eslint/parser": "^5.37.0",
		"eslint": "^8.23.1",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"openapi-typescript": "^5.4.1",
		"tough-cookie-file-store": "^2.0.3",
		"typescript": "^4.8.3",
		"vitest": "^0.23.4"
	},
	"scripts": {
		"test-all": "vitest auth && vitest api creator user video",
		"test-auth": "vitest auth",
		"test-noLogin": "vitest api creator user video",
		"postinstall": "tsc",
		"fetchSchema": "./node_modules/.bin/openapi-typescript https://raw.githubusercontent.com/Jman012/FloatplaneAPI/main/floatplane-openapi-specification.json --make-paths-enum --output ./src/lib/apiSchema.ts"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./api": {
			"types": "./dist/api.d.ts",
			"import": "./dist/api.js"
		},
		"./auth": {
			"types": "./dist/auth.d.ts",
			"import": "./dist/auth.js"
		},
		"./creator": {
			"types": "./dist/creator.d.ts",
			"import": "./dist/creator.js"
		},
		"./user": {
			"types": "./dist/user.d.ts",
			"import": "./dist/user.js"
		},
		"./cdn": {
			"types": "./dist/cdn.d.ts",
			"import": "./dist/cdn.js"
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Inrixia/floatplaneAPI.ts.git"
	},
	"author": "Inrix",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Inrixia/floatplaneAPI.ts/issues"
	},
	"homepage": "https://github.com/Inrixia/floatplaneAPI.ts#readme"
}
